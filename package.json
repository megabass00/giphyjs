{
  "name": "giphy-js",
  "version": "1.0.0",
  "description": "frontend/backend with register/login users",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "prod": "NODE_ENV=production node src/index.js",
    "start:docker": "NODE_ENV=docker node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "NODE_ENV=test mocha --timeout 5s src/test/*.test.js --exit",
    "test:watch": "NODE_ENV=test mocha --timeout 5s src/test/*.test.js --watch",
    "test:browser": "NODE_ENV=test live-server --port=9000 --open=src/test-browser.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/megabass00/giphyjs.git"
  },
  "author": "Luis Izquierdo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/megabass00/giphyjs/issues"
  },
  "homepage": "https://github.com/megabass00/giphyjs#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "colors": "^1.3.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "custom-env": "^1.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.1-alpha.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.7",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sharp": "^0.21.3",
    "slugify": "^1.3.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "errorhandler": "^1.5.0",
    "faker": "^4.1.0",
    "live-server": "^1.2.1",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.9",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2"
  }
}
